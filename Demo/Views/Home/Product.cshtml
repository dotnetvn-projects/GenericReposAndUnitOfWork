@model IEnumerable<Product>
@{
    ViewData["Title"] = "Product";
}
<div class="row">
    <h2>Product List</h2>
</div>
<div class="row">
    <table class="table table-bordered">
        <thead>
            <tr>
                <th>Id</th>
                <th>Sku</th>
                <th>Name</th>
                <th>Price</th>
                <th>Created Date</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>#@item.Id</td>
                    <td>@item.Sku</td>
                    <td>@item.Name</td>
                    <td>@item.Price</td>
                    <td>@item.CreatedDate</td>
                </tr>
            }
        </tbody>
    </table>
</div>